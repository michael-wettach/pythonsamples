				<xs:element name="data" type="xs:string" minOccurs="1" maxOccurs="1" /> 
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="encryption">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="encryptionType" minOccurs="1" maxOccurs="1">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="30" /> 
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="decryptionKey" minOccurs="1" maxOccurs="1">
					<xs:simpleType>
						<xs:restriction base="xs:base64Binary" /> 
					</xs:simpleType>
				</xs:element>
				<xs:element name="initializationVector" minOccurs="1" maxOccurs="1">
					<xs:simpleType>
						<xs:restriction base="xs:base64Binary" /> 
					</xs:simpleType>
				</xs:element>
				<xs:element name="decryptionKeyVersion" minOccurs="1" maxOccurs="1">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="30" /> 
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="customParameters">
		<xs:annotation>
			<xs:documentation>a section of key elements that contains a name value pair collection of data needed for archiving, printing and else (basically meta information about the header element) in a generic manner</xs:documentation> 
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="key" maxOccurs="unbounded" /> 
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="version">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="10" /> 
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="receiverAddress">
		<xs:annotation>
			<xs:documentation>address lines(they are printed in the order they arrive) and flags</xs:documentation> 
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="addressLine" minOccurs="3" maxOccurs="7">
					<!--  printed in the order they arrive  --> 
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="50" /> 
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="mailingAllowed" minOccurs="0" maxOccurs="1">
					<!--  printed in the order they arrive  --> 
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="1" /> 
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="key">
		<xs:annotation>
			<xs:documentation>a generic key value combination used for document meta data like archive index information and such</xs:documentation> 
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="name" type="xs:string" use="required" /> 
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="elements">
		<xs:annotation>
			<xs:documentation>a generic structural element used for payload structuring</xs:documentation> 
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="element" minOccurs="0" maxOccurs="unbounded" /> 
				<xs:element ref="elements" minOccurs="0" maxOccurs="unbounded" /> 
			</xs:sequence>
			<xs:attribute name="name" type="xs:string" use="required" /> 
			<xs:attribute name="attribute" type="xs:string" use="optional" /> 
			<!-- 
in case of searching or sorting the name of the elements in a section should be unique   --> 
		</xs:complexType>
	</xs:element>
	<xs:element name="element">
		<!-- a generic structural element used for payload structuring  --> 
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="name" type="xs:string" use="required" /> 
					<xs:attribute name="attribute" type="xs:string" use="optional" /> 
				</xs:extension>
				<!-- 
in case of searching or sorting the name of the element in a section should be unique   --> 
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="encryptedData">
		<xs:simpleType>
			<xs:restriction base="xs:base64Binary" /> 
		</xs:simpleType>
	</xs:element>
	<xs:element name="nameLines">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="nameLine" minOccurs="0" maxOccurs="3">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="35" /> 
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="streetAndNumber">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="35" /> 
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="zipCodeAndTown">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="35" /> 
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="country">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="35" /> 
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="posyKeys">
		<xs:annotation>
			<xs:documentation>posy distribution keys</xs:documentation> 
		</xs:annotation>
		<!-- posy distribution keys  --> 
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="posyDistributionKey1" minOccurs="0" maxOccurs="1" /> 
				<xs:element ref="posyDistributionKey2" minOccurs="0" maxOccurs="1" /> 
				<xs:element ref="posyDistributionKey3" minOccurs="0" maxOccurs="1" /> 
				<xs:element ref="posyDistributionKey4" minOccurs="0" maxOccurs="1" /> 
				<xs:element ref="posyDistributionKey5" minOccurs="0" maxOccurs="1" /> 
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="posyDistributionKey1">
		<xs:annotation>
			<xs:documentation>Verteilkennzeichen Post; V = versandfertig, I = interne Anschrift (z.B. AKP), B = VersandBesonderheiten (z.B. Konten mit Postversandschlüssel), A = auszusteuernde Post; X = Xchanging; Y bei PVS=AKP</xs:documentation> 
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="V" /> 
				<xs:enumeration value="I" /> 
				<xs:enumeration value="B" /> 
				<xs:enumeration value="A" /> 
				<xs:enumeration value="Y" /> 
				<xs:enumeration value="X" /> 
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="posyDistributionKey2">
		<xs:annotation>
			<xs:documentation>POSY: Postversandschlüssel; Hier können Ergänzungen zu DISTRIB_KEY1 festgehalten werden, die POSY eine weitere Differenzierung in der Stapelaufteilung ermöglicht – bis hin zum Stapelnamen selbst (z.B.: AUS6B1)</xs:documentation> 
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="6" /> 
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="posyDistributionKey3">
		<xs:annotation>
			<xs:documentation>POSY: weiteres Verteil-Kennzeichen.Filialnummer und die ersten 3 Stellen der Kontonummer</xs:documentation> 
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="6" /> 
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="posyDistributionKey4">
		<xs:annotation>
			<xs:documentation>POSY: Zusätzlicher Controlling-Begriff für weitere Abrechnungen</xs:documentation> 
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="12" /> 
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="posyDistributionKey5">
		<xs:annotation>
			<xs:documentation>POSY: Zusätzlicher Sortierbegriff z.B. in welcher Reihenfolge gedruckt werden soll, erst PLZ Frankfurt, dann Bayern etc.</xs:documentation> 
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="12" /> 
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
</xs:schema>