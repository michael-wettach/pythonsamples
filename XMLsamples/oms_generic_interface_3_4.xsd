<?xml version="1.0" encoding="UTF-8" standalone="no" ?> 
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:dsig="http://www.w3.org/2000/09/xmldsig#" attributeFormDefault="unqualified">
	<xs:import namespace="http://www.w3.org/2000/09/xmldsig#" schemaLocation="xmldsig-core-schema.xsd" /> 
	<xs:element name="docs">
		<xs:annotation>
			<xs:documentation>main tag identifying the beginning of a series of documents</xs:documentation> 
		</xs:annotation>
		<!-- main tag identifying the beginning of a series of documents  --> 
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="doc" minOccurs="0" maxOccurs="unbounded" /> 
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="doc">
		<xs:annotation>
			<xs:documentation>the single document</xs:documentation> 
		</xs:annotation>
		<!-- the single document  --> 
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="header" minOccurs="1" maxOccurs="1" /> 
				<xs:element ref="payload" minOccurs="0" maxOccurs="1" /> 
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="header">
		<xs:annotation>
			<xs:documentation>the header contains required meta data for the document as well as data that is commonly needed</xs:documentation> 
		</xs:annotation>
		<!-- 
the header contains required meta data for the document as well as 	data that is commonly needed   --> 
		<xs:complexType>
			<xs:annotation>
				<xs:documentation>if the owner is a partner, for more detailed information this element should be added</xs:documentation> 
			</xs:annotation>
			<xs:sequence>
				<xs:element ref="version" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>version of the header.</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element ref="documentInformation" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>generic but necessary and required information about this document</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element ref="inputChannelName" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>the input channel this document originally comes from</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element name="deliveryOn" type="xs:date" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>delivery date and will be used as correspondence print date</xs:documentation> 
					</xs:annotation>
					<!--  date this document was provided to the output engine (Format: YYYY-MM-DD)  --> 
				</xs:element>
				<xs:element ref="nachrichtenbox"  minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Tag used to deliver information relevant for Nachrichtenbox like subject line
						</xs:documentation>
					</xs:annotation>					
				</xs:element>
				<xs:element ref="partner" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>the information about partner</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element ref="letterSalutations" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>a list of max 2 letter salutation(Briefanrede)</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element ref="receiverAddress" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>address lines(they are printed in the order they arrive) and flags</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element ref="accountConsultant" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>the information about account consultant</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element ref="printerData" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Information about the printer if the document will be printed directly to a printer</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element ref="emailData" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Information about the Email if the document will be sent to an email address</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element name="attachments" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>main tag identifying the beginning of a series of attachments</xs:documentation> 
					</xs:annotation>
					<!-- main tag identifying the beginning of a series of attachments  --> 
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="attachment" minOccurs="0" maxOccurs="unbounded" /> 
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element ref="qrCode" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Data to generate QR Code to be printed in document</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element ref="dataMatrix" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Data to generate a data matrix Code to be printed in document</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element ref="audit" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>information about audit</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element ref="encryption" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>information about encryption</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element ref="posyKeys" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>posy distribution keys for dbPrint</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element ref="accountInfo" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>account info</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element name="digitalDeliveryOnly" type="xs:boolean" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>flag to control document processing for POSTBOX only</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element ref="partnerDataExclusion" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>flags for excluding specific parts of partner data replica</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element ref="subjectPostbox" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Postbox subject line</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="customParameters" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>header custom parameter. collection of data needed for archiving, printing and else in a generic manner</xs:documentation> 
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="payload">
		<xs:annotation>
			<xs:documentation>this section contains the actual document raw data value that needs to be formatted</xs:documentation> 
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="elements" minOccurs="0" maxOccurs="unbounded" /> 
				<xs:element ref="encryptedData" minOccurs="0" maxOccurs="1" /> 
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="documentInformation">
		<xs:annotation>
			<xs:documentation>generic but necessary and required information about this document</xs:documentation> 
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="trackingId" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>identifies the actual doc via a unique id to be stored in document metadata for tracking</xs:documentation> 
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="60" /> 
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="docTypeId" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>identifies the actual doc type via a unique id</xs:documentation> 
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="10" /> 
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element ref="version" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>identifies the working version of the document's template to use</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element name="docTypeDescription">
					<xs:annotation>
						<xs:documentation>describe the document</xs:documentation> 
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="200" /> 
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="documentType" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>describe the document type, business template</xs:documentation> 
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1" /> 
							<xs:maxLength value="4" /> 
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="languageReceiver" default="de" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>the language the receiver want to have this document in ( ISO 639-1:2002)</xs:documentation> 
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:pattern value="[a-z,A-Z]{2}" /> 
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="countryReceiver" default="DE" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>country the document receiver lives in (ISO 3166 ALPHA-2)</xs:documentation> 
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:pattern value="[A-Z]{2}" /> 
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="documentCharacteristic" default="O" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>identifies if this document is an original,a duplicate or else</xs:documentation> 
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="O" /> 
							<xs:enumeration value="D" /> 
							<xs:enumeration value="Z" /> 
                            <xs:enumeration value="V" /> 
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="copyCounter" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>identifies how many copies should be created, specially for documentCharacteristic "D"</xs:documentation> 
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:int">
							<xs:minInclusive value="0" /> 
							<xs:maxInclusive value="99" /> 
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="envelopeId" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>identifies whether this document may be bundled with another one; if two due documents have the same id they will be bundled</xs:documentation> 
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="64" /> 
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element ref="customParameters" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>document custom parameter. collection of data in a generic manner</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element name="productId" type="xs:string" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>product identifier</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element name="prodDesc" type="xs:string" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>product description</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element name="productHandlingId" type="xs:string" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>product handling identifier</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element name="productMarketingId" type="xs:string" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>product marketing identifier</xs:documentation> 
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="inputChannelName">
		<xs:annotation>
			<xs:documentation>the name of data provider</xs:documentation> 
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="10" /> 
			</xs:restriction>
		</xs:simpleType>
		<!-- the input channel this document originally comes from  --> 
	</xs:element>
	<xs:element name="nachrichtenbox">
		<xs:annotation>
			<xs:documentation> Tag used to deliver information relevant for Nachrichtenbox like subject line</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="subject" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Betreff zeile für Nachrichtenbox</xs:documentation> 
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="255" /> 
						</xs:restriction>
					</xs:simpleType>					
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="partner">
		<xs:annotation>
			<xs:documentation>partnter information block</xs:documentation> 
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="clientId" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>identifies the legal entity of the client, i. e. 002 (PGK), 003 (DBAG), 014 (norisbank), 500 (SOP) or else</xs:documentation> 
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:pattern value="[0-9]{3}" /> 
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="brandId" type="xs:string" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Brandingmerkmal</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element name="partnerId" type="xs:string" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>the id of the partner(in PD)</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element name="receiverPartnerId" type="xs:string" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>the id of the partner(in Business Partner)</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element name="businessArea" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>the business area of partner account</xs:documentation> 
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:pattern value="[0-9]{3}" /> 
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="division" type="xs:string" minOccurs="0" maxOccurs="1"> 
					<xs:annotation>
						<xs:documentation>division</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element name="subDivision" type="xs:string" minOccurs="0" maxOccurs="1"> 
					<xs:annotation>
						<xs:documentation>sub division</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element name="corrDivision" type="xs:string" minOccurs="0" maxOccurs="1"> 
					<xs:annotation>
						<xs:documentation>corr division</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element name="firstCustContract" type="xs:string" minOccurs="0" maxOccurs="1"> 
					<xs:annotation>
						<xs:documentation>first customer contract</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element name="contractNo" type="xs:string" minOccurs="0" maxOccurs="1"> 
					<xs:annotation>
						<xs:documentation>contract number</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element name="branchNo" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>the branch number of partner account</xs:documentation> 
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:pattern value="[0-9]{3}" /> 
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="accountNo" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>the account number of partner account</xs:documentation> 
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="20" /> 
							<xs:minLength value="7" /> 
							<xs:pattern value="[0-9]*" /> 
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="subAccountNo" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>the subaccount number of partner account</xs:documentation> 
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:pattern value="[0-9]{2}" /> 
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<!--  changed from mandatory V2.4 to optional V2.5  --> 
				<xs:element name="currency" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>the currency of partner account</xs:documentation> 
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:pattern value="[A-Z]{3}" /> 
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="bankCode" type="xs:string" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>the national (German) bank code (Bankleitzahl)</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element name="bankCountry" type="xs:string" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>country of bank</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element name="iban" type="xs:string" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>the international bank account number (IBAN)</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element name="bic" type="xs:string" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>the international bank code (BIC)</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element name="accountOwner" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>titel, first and lastname of account owner</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element name="accountOwnerLastName" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>lastname and title of account owner</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element name="accountOwnerLastNameOnly" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>lastname of account owner</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element name="accountOwnerFirstName" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>firstname and title of account owner</xs:documentation> 
					</xs:annotation>
                </xs:element>
				<xs:element name="accountOwnerFirstNameOnly" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>firstname of account owner</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element name="employeeFlag" default="0" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>0:false and 1:true</xs:documentation> 
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="0" /> 
							<xs:enumeration value="1" /> 
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="internalFlag" default="0" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>0:false for external address and 1:true for internal address</xs:documentation> 
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="0" /> 
							<xs:enumeration value="1" /> 
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element ref="streetAndNumber" minOccurs="0" maxOccurs="1"> 
					<xs:annotation>
						<xs:documentation>street and number</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element ref="zipCodeAndTown" minOccurs="0" maxOccurs="1"> 
					<xs:annotation>
						<xs:documentation>zip code and town</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element ref="country" minOccurs="0" maxOccurs="1"> 
					<xs:annotation>
						<xs:documentation>country</xs:documentation> 
					</xs:annotation>
				</xs:element>
				<xs:element name="bankProductType" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>PD_CONTRACT.bank_product_id</xs:documentation> 
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="2" /> 
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="partnerType" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>PD_PARTNER.partner_type</xs:documentation> 
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="3" /> 
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="gender" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>PD_PARTNER.sex</xs:documentation> 
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="1" /> 
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="birthName" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>PD_PARTNER.BirthName</xs:documentation> 
					</xs:annotation>
